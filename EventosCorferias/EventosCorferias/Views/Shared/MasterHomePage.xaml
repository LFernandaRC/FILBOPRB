<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:EventosCorferias.Views.Usuario"
            xmlns:local="clr-namespace:EventosCorferias.Views.Suceso"
            x:Class="EventosCorferias.Views.Usuario.MasterHomePage"
            NavigationPage.HasNavigationBar="false">

    <FlyoutPage.Flyout>
        <views:MenuPage/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:SucesoView/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>